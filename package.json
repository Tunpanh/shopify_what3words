{
  "name": "test-shop",
  "private": true,
  "version": "0.1.0",
  "description": "Shopify Remix app scaffold with Checkout UI Extension workspace",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "remix build",
    "dev": "remix dev",
    "dev:shopify": "shopify app dev",
    "start": "remix-serve ./build/index.js",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0",
    "typecheck": "tsc --pretty --noEmit",
    "typecheck:extension": "tsc --pretty --noEmit -p extensions/w3w-checkout-ui/tsconfig.json"
  },
  "dependencies": {
    "@prisma/client": "^6.4.1",
    "@remix-run/node": "^2.16.0",
    "@remix-run/react": "^2.16.0",
    "@remix-run/serve": "^2.16.0",
    "@shopify/app-bridge-react": "^4.1.6",
    "@shopify/polaris": "^13.9.5",
    "@shopify/shopify-api": "^11.11.1",
    "@shopify/shopify-app-remix": "^3.7.2",
    "isbot": "^5.1.23",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.16.0",
    "@types/node": "^22.13.4",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "eslint": "^9.20.1",
    "eslint-plugin-react-hooks": "^5.1.0",
    "prisma": "^6.4.1",
    "typescript": "^5.7.3"
  }
}
